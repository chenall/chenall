		{
			return null; 
		}
		res = http.get("http://www.dbank.com/app/web/weibo.php?action=getLoginUrl&oauth_type=sina");
		res = string.sub(res,2,-2);
		res = string.replace(res,"\\/","/");
		wb.go(res);
		wb.wait();
	}
	else {
		res = http.get("http://login.dbank.com/loginauth.php?nsp_app=48049")
		var nonce = string.match( res,'nonce[^>]*value=\"([^"]*)\"');
		var nsp_cid = string.match(res,'nsp_cid[^>]*value=\"([^"]*)\"')
