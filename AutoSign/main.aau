code_c = /****
	char *xorstr (char *msg)
	{   
   		unsigned char *p = msg+1;
   		while (*p)
   		{
   			*p^=*msg;
   			if (*p <= '\'') *p^='e';
   			++p;
   		}
		return msg+1; 
	} 
****/

vm = tcc( ); 
 
//编译C源码
vm.compile(code_c); 
 
//获取C函数
password_enc = vm.getCdecl("xorstr","string(string msg)")
password_dec = password_enc

