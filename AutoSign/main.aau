		{
			http.get("http://login.dbank.com/accounts/logout");//退出;
			return {1;sp};
		}
		sp = res['k'];
		http.get("http://login.dbank.com/loginauth.php?k="+sp);
		//{"retcode":"0000","retdesc":"用户签到成功","type":2,"space":37}
		//{"retcode":"0001","retdesc":"用户已签到"}
	}
	//36f2f83c44c3a29b98d64417dbf75fd3nsp_cb_jqjspnsp_fmtJSnsp_sid9uuu1w7uCr8hkua.WjouavD4xYuIsGMH2ZO4BnY-N.7VXSUfnsp_svcnsp.user.getAccountsnsp_ts1340702904908type1
	var Cookie = inet.getCookie("http://www.dbank.com");
	var nsp_sid = string.match(Cookie,"session=([^;]*)");
	var secret = string.match(Cookie,"secret=([^;]*)");
	var nsp_ts = tonumber(time());
	var nsp_key;
	if (flags == -1)
	{
		try{
			lstbox_add("获取用户帐号:")
			nsp_key = secret + "nsp_cb_jqjspnsp_fmtJSnsp_sid"+nsp_sid+"nsp_svcnsp.user.getAccountsnsp_ts"+nsp_ts+"type1";
			nsp_key = string.crypt.md5(nsp_key)
			//获取用户帐号信息
			res = http.get("http://api.dbank.com/rest.php?nsp_svc=nsp.user.getAccounts&type=1&nsp_sid="+ nsp_sid + "&nsp_ts="+nsp_ts+"&nsp_key="+nsp_key+"&nsp_fmt=JS&nsp_cb=_jqjsp");
			res = string.match(res,'"account":"(.*)"');
			lstbox_add(res);
		}
		catch(e){
			lstbox_add("出错了:"+Cookie);
			res = null;
		}
		if (res == null)
			return null;
		wb.go("http://login.dbank.com/accounts/logout");//退出;
		return {2;res};
	}
	nsp_key = secret + "anticache205nsp_sid"+nsp_sid+"nsp_svccom.dbank.signin.signinnsp_ts"+nsp_ts;
	nsp_key = string.crypt.md5(nsp_key);
	nsp_key = "http://api.dbank.com/rest.php?anticache=205&nsp_svc=com.dbank.signin.signin"+"&nsp_ts="+nsp_ts+"&nsp_sid="+nsp_sid+"&nsp_key="+nsp_key;
	lstbox_add("自动签到URL:"+nsp_key);
	var result = http.get(nsp_key);
	var ret = null
	try{
		res = web.json.parse(result);
		if (res['retcode'] == "0000"){
			ret = res['space'] + "MB"
			lstbox_add("签到成功获得空间"+ret);
		}
		elseif(res['retcode'] == "0001"){
			ret = "已签到"
			lstbox_add(string.fromto(res['retdesc']))
		}
	}
	catch(e){
		lstbox_add("出现错误以下是相关数据：")
		lstbox_add(result);
	}

  	return ret;
}


